= will_paginate rounds
%table#rounds
  %thead
    %tr
      %th Course
      %th Played On
      %th Score
      %th Differential
      %th
      %th
  %tbody
    - rounds.each_with_index do |r, i|
      %tr
        %td= r.course
        %td= r.played_on
        %td= r.score
        %td= r.differential
        %td= link_to 'Edit', edit_round_path(r)
        %td= link_to 'Delete', round_path(r), :method => 'delete'